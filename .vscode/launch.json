{
    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "chrome",
        "url": "http://localhost:8080",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Debug Android",
        "request": "launch",
        "type": "reactnative",
        "platform": "android",
        "sourceMaps": true,
        "cwd": "${workspaceFolder}"
      },
      {
        "name": "Debug iOS",
        "request": "launch",
        "type": "reactnative",
        "platform": "ios",
        "sourceMaps": true,
        "cwd": "${workspaceFolder}"
      },
      {
        "name": "Debug Expo",
        "request": "launch",
        "type": "reactnative",
        "platform": "exponent",
        "sourceMaps": true,
        "cwd": "${workspaceFolder}"
      },
      {
        "name": "Attach to packager",
        "request": "attach",
        "type": "reactnative",
        "address": "localhost",
        "port": 19001,
        "localRoot": "${workspaceFolder}",
        "remoteRoot": "",
        "sourceMaps": true,
        "cwd": "${workspaceFolder}"
      }
    ]
  }